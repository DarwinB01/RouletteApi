package com.roulette.api.dto;

public class WinnerDTO {

	private double acquiredMoney;
	private String idUser;

	public WinnerDTO() {
		super();
	}

	public WinnerDTO(String idUser, Long acquiredMoney) {
		this.idUser = idUser;
		this.acquiredMoney = acquiredMoney;
	}

	public Long getAcquiredMoney() {

		return acquiredMoney;
	}

	public void setAcquiredMoney(Long acquiredMoney) {
		this.acquiredMoney = acquiredMoney;
	}

	public String getIdUser() {

		return idUser;
	}

	public void setIdUser(String idUser) {
		this.idUser = idUser;
	}

}
